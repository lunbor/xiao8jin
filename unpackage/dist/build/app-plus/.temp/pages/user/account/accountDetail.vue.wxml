        <view class="_view data-v-965e65f2 list contentbg by-mb-30">
          <view class="_view data-v-965e65f2 list-item pd20">
            <view class="_view data-v-965e65f2 list-time font-sg">支付通道:
              <text class="_text data-v-965e65f2">{{accountDetail.title}}</text>
            </view>
            <view class="_view data-v-965e65f2 list-time font-sg">提交时间:
              <text class="_text data-v-965e65f2">{{accountDetail.ctime}}</text>
            </view>
            <view class="_view data-v-965e65f2 list-order font-sg">交易单号:
              <text class="_text data-v-965e65f2">{{accountDetail.order_no}}</text>
            </view>
            <view class="_view data-v-965e65f2 list-order font-sg">付款账户:
              <text class="_text data-v-965e65f2">{{accountDetail.credit_code}}</text>
            </view>
            <view class="_view data-v-965e65f2 list-order font-sg">收款账户:
              <text class="_text data-v-965e65f2">{{accountDetail.bank_card}}</text>
            </view>
            <view class="_view data-v-965e65f2 list-time font-sg">交易备注:
              <text class="_text data-v-965e65f2 red-text">{{accountDetail.dscInc}}</text>
            </view>
          </view>
        </view>
      </view>
      <view class="_view data-v-965e65f2 content pt30">
        <view class="_view data-v-965e65f2 list contentbg">
          <view class="_view data-v-965e65f2 uni-flex uni-row repay-fee" style="justify-content: space-between;">
            <view class="_view data-v-965e65f2 text uni-list-cell-left">收款金额</view>
            <view class="_view data-v-965e65f2 text uni-list-cell-left red-text">￥{{accountDetail.money}}</view>
          </view>
          <view class="_view data-v-965e65f2 uni-flex uni-row repay-fee" style="justify-content: space-between;">
            <view class="_view data-v-965e65f2 text uni-list-cell-left">手续费</view>
            <view class="_view data-v-965e65f2 text uni-list-cell-left">￥{{accountDetail.fee}}</view>
          </view>
          <view class="_view data-v-965e65f2 uni-flex uni-row repay-fee" style="justify-content: space-between;">
            <view class="_view data-v-965e65f2 text uni-list-cell-left">代付费</view>
            <view class="_view data-v-965e65f2 text uni-list-cell-left">￥{{accountDetail.mercfee}}</view>
