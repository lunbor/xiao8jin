<template name="39f4d89e">
  <view class="_view data-v-4885c668 uni-common-pb">
    <view class="_view data-v-4885c668 uni-login-logo">
      <image src="../../../static/login-logo.png" class="_image data-v-4885c668"></image>
    </view>
    <view class="_view data-v-4885c668 uni-padding-wrap">
      <form bindsubmit="handleProxy" data-eventid="{{'74r-5'}}" data-comkey="{{$k}}" class="_form data-v-4885c668">
        <view class="_view data-v-4885c668 uni-form-item uni-column">
          <view class="_view data-v-4885c668 with-fun by-login-input">
            <input class="_input data-v-4885c668 uni-input" type="number" name="account" placeholder="请填写注册的手机号" value="{{inputClearValue}}" bindinput="handleProxy" data-eventid="{{'7OB-0'}}" data-comkey="{{$k}}" />
            <view class="_view data-v-4885c668 uni-icon uni-icon-clear" wx:if="{{showClearIcon}}" bindtap="handleProxy" data-eventid="{{'L8D-1'}}" data-comkey="{{$k}}"></view>
          </view>
        </view>
        <view class="_view data-v-4885c668 uni-form-item uni-column">
          <view class="_view data-v-4885c668 with-fun by-login-input">
            <input class="_input data-v-4885c668 uni-input" type="number" name="smsCode" placeholder="请填入验证码" value="{{smsCode}}" bindinput="handleProxy" data-eventid="{{'Re2-2'}}" data-comkey="{{$k}}" />
            <view class="_view data-v-4885c668 text by-dj-btn {{is_sms ? '' : 'active'}}" bindtap="handleProxy" data-eventid="{{'NkA-3'}}" data-comkey="{{$k}}">{{fsbtn}}</view>
          </view>
        </view>
        <view class="_view data-v-4885c668 uni-form-item uni-column">
          <view class="_view data-v-4885c668 with-fun by-login-input">
            <input class="_input data-v-4885c668 uni-input" placeholder="请填写6~18位字符新密码" name="password" password="{{showPassword}}" />
            <view class="_view data-v-4885c668 uni-icon uni-icon-eye {{[!showPassword ? 'uni-active' : '']}}" bindtap="handleProxy" data-eventid="{{'7LX-4'}}" data-comkey="{{$k}}"></view>
          </view>
        </view>
        <view class="_view data-v-4885c668 uni-btn-v">
          <button formType="submit" loading="{{loading}}" type="warn" class="_button data-v-4885c668">修改密码</button>
        </view>
      </form>
    </view>
  </view>
</template>