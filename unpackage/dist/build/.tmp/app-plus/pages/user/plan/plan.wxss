.already_plan{padding: 0 30rpx;margin-bottom:40rpx;}
.already_plan_card {
	border-radius: 12rpx;
	-webkit-box-shadow: 4rpx 4rpx 20rpx 4rpx rgba(0, 37, 174, 0.2);
	box-shadow: 4rpx 4rpx 20rpx 4rpx rgba(0, 37, 174, 0.2);
	background-color: #fff;
	margin-bottom: 30rpx;
	padding: 10rpx 20rpx 0;
}
.info_plan{color: #333!important;}
.btn-repay{color: #5a91fe;font-weight: 600;text-align: center;border-top: 1px solid #ececec;padding: 8rpx 0;}
.card_style{
	background: #FFFFFF;
	border-radius: 14rpx;
	color: #333;
	margin-top: 0rpx;
	position: relative;
	border-radius: 12rpx;
	-webkit-box-shadow: 4rpx 4rpx 20rpx 4rpx rgba(0, 37, 174, 0.2);
	box-shadow: 4rpx 4rpx 20rpx 4rpx rgba(0, 37, 174, 0.2);
	text-align: left;
	padding:4px 30rpx;
}
.bank_type{font-size: 22rpx;border: 1px solid #ececec;color: #999999;border-radius: 6rpx;line-height: 1;padding: 0 10rpx;height: 30rpx;line-height: 30rpx;margin-left: 12rpx;
	margin-top: 10rpx;}
.card_bank{padding: 10rpx 0;border-bottom: 1px solid #ECECEC;overflow: hidden;}
.card_bank text{float: left;}
.card_bank_icon{width:32rpx;height:32rpx;margin-right: 10rpx;float: left;margin-top: 8rpx;}
.card_bank_info{padding: 20rpx 0;color: #333;}
.card_bank_info .name{color: #999999;}
.card_bank_info .text{margin-right: 30rpx;}
.untying_btn{width: 100rpx;height: 100rpx;background-color: rgba(255,161,60,0.2);border-radius: 500px;color: #FFFFFF;text-align: center;padding: 16rpx;float: right;}
.untying_btn text{width: 100rpx;height:100rpx;background-color: rgba(255,161,60,1);border-radius: 500px;display: inline-block;}
.card_num{font-size: 24rpx;color: #666666;margin-top: 4rpx;}
.card_main{font-size: 32rpx;font-weight: 500;padding-top: 20rpx;}
.card_main .name{font-size: 24rpx;color: #999;margin-top: 10rpx;}

