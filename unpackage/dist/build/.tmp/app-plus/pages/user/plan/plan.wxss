.uni-card-red {
		background: -webkit-gradient(linear, left top, right top, from(#ff7575), to(#fe5950));
		background: -o-linear-gradient(left, #ff7575, #fe5950);
		background: linear-gradient(to right, #ff7575, #fe5950);
		border-radius: 15rpx;
		color: #fff;
		min-height: 260rpx;
		margin-top: 30rpx;
		overflow: hidden;
		position: relative;
}
.uni-card .lf7 {
		width: 74%;
		font-weight: 500;
		font-size: 32rpx;
		text-align: left;
		-webkit-flex-direction: inherit;
		-ms-flex-direction: inherit;
		-webkit-box-orient: inherit;
		-webkit-box-direction: inherit;
		        flex-direction: inherit;
		line-height: 60rpx;
}
.uni-card .lf7 text {
		font-size: 28rpx;
		font-weight: normal;
		line-height: 60rpx;
		margin-left: 10rpx;
}
.uni-card .rg3 {
		width: 26%;
}
.uni-card .lf {
		font-weight: 500;
		font-size: 32rpx;
		text-align: left;
		line-height: 60rpx;
}
.uni-card .lf text {
		font-size: 28rpx;
		line-height: 40rpx;
		display: block;
}
.uni-card .rg {
		margin-left: 20rpx;
}
.uni-card .code-fj {
		line-height: 90rpx;
		height: 90rpx;
		font-size: 38rpx;
		width: 100%;
		text-align: right;
		-webkit-box-pack: end;
		-webkit-justify-content: flex-end;
		    -ms-flex-pack: end;
		        justify-content: flex-end;
}
.uni-card .code-fj .text {
		margin-right: 50rpx;
		text-align: right;
		line-height: 105rpx;
		font-size: 38rpx;
}
.uni-card .code-fj .text.lhg50 {
		line-height: 90rpx;
}
.mini-btn {
		line-height: 55rpx;
		width: 160rpx;
		height: 55rpx;
		background-color: #fe5950 !important;
		background: transparent !important;
}
.mini-btn:after {
		border: 5rpx solid #FFFFFF;
}
.linebg {
		background: #EEEEEE;
