
page {
		height: auto;
		min-height: 100%;
		background-color: #FFFFFF;
}
.vip_bg{ height: 100rpx; background-color: #3c3f4c;padding:  30rpx;}
.vip_user{color: #d0be9a; margin-left: 30rpx;  font-size: 28rpx;color: #fff;float: left;font-weight: 500;}
.vip_time{color: #f4e4bc; font-size: 24rpx;}
.head-pic{width: 80rpx;height: 80rpx;border-radius: 500px;float: left;margin-top: 10rpx;}
.head-pic image{width: 80rpx;height: 80rpx;border-radius: 500px;}
.head-pic ._img{width: 80rpx;height: 80rpx;border-radius: 500px;}
.postage{margin: 30rpx;margin-top: 20rpx;}
.vip_title{font-size: 32rpx;font-weight: 800;color: #333;padding-bottom: 30rpx;
}
.vip-money{
		border: 2rpx solid #DDDDDD;
		border-radius: 12rpx;
		height: 180rpx;
		padding: 30rpx 0 20rpx;
		text-align: center;
		background-color: #FFFFFF;
		width: 31%;
		position: relative;
}
.vip-money:nth-child(2){margin: 0 3.5%;}
.vip-money.active{
		border: 2rpx solid #ebc58c;
		background-color: #fffaf2;
}
.vip_icon{width: 120rpx;height: 34rpx;background: url(data:image/png;base64,) no-repeat;background-size: 100% 100%;position: absolute;top: -20rpx;left: 0;color: #eacc9b;
	font-size: 22rpx;text-align: center;line-height: 34rpx;}
.vip-money-title{color: #8d8c8a;font-size: 32rpx; font-weight:900;
}
.vip-money-val{color: #a57459;font-size: 32rpx; font-weight: 900;}
.vip-money-val text{font-size: 48rpx;}
.vip-money-gqval{color: #c0c0c0;font-size: 28rpx; font-weight: normal;text-decoration:line-through;}
.vip-money.active .vip-money-title{color: #8d8c8a;
}
.vip-money.active .vip-money-gqval{color: #969696;}
.vip-money-yj{font-size: 400;font-size: 24rpx;color: #ababab;}
.vip_title ._img{height: 136rpx;width: 100%;}
.tq-title{background: url(data:image/png;base64,) no-repeat;background-size: 100% 100%;height: 136rpx;}
.vip_info{margin:0 30rpx}
.linebg{ margin: 30rpx 0rpx; height: 4rpx; background-color: #ebebeb; width: 90%; margin: 0rpx auto;}
.tq-center{border:1px solid #ececec;border-top:none}
.vip_info_case{border-bottom: 1px solid #ECECEC;}
.vip_info_name{width: 18%;text-align: center;font-size: 26rpx;border-right: 1px solid #ECECEC;padding-top: 20rpx;}
.vip_info_main{-webkit-box-flex: 1;-webkit-flex: 1;-ms-flex: 1;flex: 1;padding:10rpx 30rpx;}
.vip_info_main uni-view{font-size: 24rpx;color: #666;}
.vip_info_main uni-view text{color: #ffa13c;}
.by-button-submit{background-color: #efdcb2;
			background: -webkit-gradient(linear, left top, right top, from(#efdcb2), to(#d5b683));
			background: -o-linear-gradient(left, #efdcb2, #d5b683);
			background: linear-gradient(left, #efdcb2, #d5b683);
			border-radius: 500px;
			-webkit-box-shadow: 4rpx 4rpx 20rpx 4rpx rgba(215, 185, 135, 0.4);
			box-shadow: 4rpx 4rpx 20rpx 4rpx rgba(215, 185, 135, 0.4);margin: 50rpx 0;}
.pay_case{border: 1px solid #ECECEC;border-radius: 12rpx;padding: 0 30rpx;}
.alipay{ text-align: center; padding: 40rpx;height: 619rpx;}
.alipay image{ width: 444rpx; height: 619rpx;}
.alipay ._img{ width: 444rpx; height: 619rpx;}
.weixin{ text-align: center; padding: 40rpx;height: 619rpx;}
.weixin image{ width: 444rpx; height: 537rpx;}
.weixin ._img{ width: 444rpx; height: 537rpx;}
.font-sg{ font-weight: normal; font-family: pingfangxi;}
.red-text{ color: #fe5950;}
.tip_btn{color: #cdd1dd; text-align: center;}
.tip_btn text{color: #fd5950;}
	/* 遮罩层 */
.uni-mask {
		background: rgba(0, 0, 0, 0.6);
		position: fixed;
		width: 100%;
		height: 100%;
		left: 0;
		top: 0;
		z-index: 1;
}
.tip_cancel {
		text-align: center;
}
.tip_cancel image {
		width: 70rpx;
		height: 123rpx;
}
.add-card-btn {
		font-size: 28rpx;
		font-weight: 500;
		line-height: 100rpx;
		height: 100rpx;
}
.add-card-btn .add-title {
		line-height: 96rpx;
		font-size: 28rpx;
}
.uni-icon-plusempty {
		font-size: 68rpx;
		line-height: 100rpx;
}

